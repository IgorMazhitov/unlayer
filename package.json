{
  "name": "@unlayer/svelte-email-editor",
  "version": "1.0.0",
  "type": "module",
  "description": "Svelte wrapper for Unlayer Email Editor",
  "keywords": [
    "svelte",
    "svelte-email",
    "unlayer",
    "email-editor",
    "email-builder",
    "drag-drop",
    "visual-editor"
  ],
  "author": "",
  "repository": {
    "type": "git",
    "url": "https://github.com/IgorMazhitov/unlayer.git"
  },
  "bugs": {
    "url": "https://github.com/IgorMazhitov/unlayer/issues"
  },
  "homepage": "https://github.com/IgorMazhitov/unlayer#readme",
  "files": [
    "dist",
    "README.md"
  ],
  "main": "./dist/unlayer-svelte.js",
  "module": "./dist/unlayer-svelte.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "svelte": "./dist/index.js",
      "default": "./dist/unlayer-svelte.js"
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:lib": "vite build --mode library",
    "preview": "vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "prepublishOnly": "npm run build:lib"
  },
  "devDependencies": {
    "@sveltejs/vite-plugin-svelte": "^4.0.0",
    "@tsconfig/svelte": "^5.0.0",
    "@types/node": "^20.0.0",
    "svelte": "^5.0.0",
    "svelte-check": "^4.0.0",
    "terser": "^5.0.0",
    "tslib": "^2.6.0",
    "typescript": "^5.0.0",
    "vite": "^5.0.0",
    "vitest": "^1.0.0"
  },
  "peerDependencies": {
    "svelte": "^5.0.0"
  }
}